{
    "extends": [
        "eslint:recommended"
    ],
    "env":{
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "eqeqeq":"warn",
        "curly":"warn",
        "no-implied-eval":"error",
        "no-eval":"error",
        "no-console":"warn",
        "quotes":["warn", "single"],
        "no-unused-vars":"warn"
    },
    "parserOptions":{
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "parser": "babel-eslint"
}